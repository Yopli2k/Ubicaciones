<view>
    <columns>
        <column name="code" display="none" order="100">
            <widget type="text" fieldname="id" />
        </column>
        <column name="warehouse" order="110">
            <widget type="text" fieldname="codewarehouse" />
        </column>
        <column name="product" order="120">
            <widget type="text" fieldname="referenceproduct" />
        </column>
        <column name="reference" order="130">
            <widget type="text" fieldname="reference" />
        </column>
        <column name="barcode" order="140">
            <widget type="text" fieldname="barcode" />
        </column>
        <column name="attribute-value-1" order="140">
            <widget type="text" fieldname="nameattribute1" />
        </column>
        <column name="attribute-value-2" order="150">
            <widget type="text" fieldname="nameattribute2" />
        </column>
        <column name="aisle" order="160">
            <widget type="text" fieldname="aisle" />
        </column>
        <column name="rack" order="170">
            <widget type="text" fieldname="rack" />
        </column>
        <column name="shelf" order="180">
            <widget type="text" fieldname="shelf" />
        </column>
        <column name="drawer" order="190">
            <widget type="text" fieldname="drawer" />
        </column>
        <column name="storage-type" title="type" display="center" order="200">
            <widget type="select" fieldname="storagetype" translate="true">
